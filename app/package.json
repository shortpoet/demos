{
  "scripts": {
    "========= Util": "",
    "git": "ts-node --esm scripts/wrangle.ts",
    "serve": "cross-env PORT=3333 tsx watch ./server/index.ts",
    "build": "vite build",
    "========= Dev": "",
    "dev": "concurrently \"pnpm serve\" \"vite --open\"",
    "w:dev": "wrangler dev --env dev --local --persist",
    "w:prev": "pnpm build --mode preview && wrangler dev --env dev  --local --persist",
    "w:dev:kv": "wrangler dev --env dev --experimental-local --experimental-local-remote-kv",
    "w:prev:kv": "pnpm build --mode preview && wrangler dev --env dev  --experimental-local --experimental-local-remote-kv",
    "w:host": "pnpm build --mode preview && wrangler dev --env dev ",
    "========= Deploy": "",
    "w:pub": "pnpm git --env uat && pnpm build --mode uat && wrangler publish --env dev ",
    "w:pub:p": "pnpm git --env prod && pnpm build:p --mode prod && wrangler publish --env prod",
    "========= Test": "",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:all": "pnpm run test && pnpm run test:e2e",
    "coverage": "vitest run --coverage",
    "w:test:e2e": "ts-node ./test/.test-wrangler.test.ts"
  },
  "dependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/server-renderer": "^3.2.33",
    "@vueuse/core": "^9.13.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "sirv": "^2.0.2",
    "ts-node": "^10.9.1",
    "vite": "^4.0.3",
    "vite-plugin-ssr": "^0.4.93",
    "vue": "^3.2.33"
  },
  "type": "module",
  "devDependencies": {
    "@brillout/test-e2e": "^0.4.17",
    "@cloudflare/kv-asset-handler": "^0.3.0",
    "@cloudflare/workers-types": "^4.20230307.0",
    "@types/node": "^18.15.1",
    "@vitest/coverage-c8": "^0.29.2",
    "@vue/test-utils": "^2.3.1",
    "concurrently": "^8.0.1",
    "cypress": "^12.7.0",
    "jsdom": "^21.1.1",
    "miniflare": "^2.12.1",
