{
  "scripts": {
    "========= Util": "",
    "git": "ts-node --esm scripts/wrangle.ts",
    "serve": "cross-env PORT=3333 tsx watch ./server/index.ts",
    "build": "vite build",
    "build:w": "node scripts/build-worker.mjs --entry ./worker/index.ts --out ./build/worker/worker.mjs",
    "build:wp": "pnpm build --mode preview && pnpm build:w",
    "========= Dev": "",
    "dev": "concurrently \"pnpm serve\" \"vite\"",
    "w:dev": "wrangler dev --env dev --local --persist",
    "w:prev": "pnpm build --mode preview && wrangler dev --env dev  --local --persist",
    "w:dev:kv": "wrangler dev --env dev --experimental-local --experimental-local-remote-kv",
    "w:prev:kv": "pnpm git --env preview && pnpm build:wp && wrangler dev --env dev  --experimental-local --experimental-local-remote-kv",
    "w:host": "pnpm build:wp --mode preview && wrangler dev --env dev ",
    "========= Deploy": "",
    "w:pub": "pnpm git --env uat && pnpm build:wp --mode uat && wrangler publish --env dev ",
    "w:pub:p": "pnpm git --env prod && pnpm build:wp --mode prod && wrangler publish --env prod",
    "========= Test": "",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:all": "pnpm run test && pnpm run test:e2e",
    "coverage": "vitest run --coverage",
    "w:test:e2e": "ts-node ./test/.test-wrangler.test.ts"
  },
  "dependencies": {
